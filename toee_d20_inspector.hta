			<table id="controls">
				<tbody>
					<tr>
						<th class="blank">Path to &quot;protos.tab&quot;:</th>
						<td class="blank"><input type="text" value="To do." size="46" />&nbsp;<input type="button" value="Load" onclick="alert('To do.')" />&nbsp;<input type="button" value="Overwrite" onclick="alert('To do.')" /></td>
					</tr>
					<tr>
						<th class="blank">Path to &quot;description.mres&quot;:</th>
						<td class="blank"><input type="text" value="To do." size="46" /></td>
					</tr>
				</tbody>
			</table>
		</div>
		<div id="outer_box_2">
			<table id="data_table" frame="box" rules="all" border="0" cellpadding="2" cellspacing="2">
				<tbody id="front_table" style="display:none;"></tbody>
			</table>
			<h2 id="load_text" style="display:block;">Loading...</h2>
